#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Pichau/Desktop/MBA/Projects/3. Desenvolvimento Back-end/04-clean-ddd/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules/@biomejs/biome/bin/node_modules:/mnt/c/Users/Pichau/Desktop/MBA/Projects/3. Desenvolvimento Back-end/04-clean-ddd/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules/@biomejs/biome/node_modules:/mnt/c/Users/Pichau/Desktop/MBA/Projects/3. Desenvolvimento Back-end/04-clean-ddd/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules/@biomejs/node_modules:/mnt/c/Users/Pichau/Desktop/MBA/Projects/3. Desenvolvimento Back-end/04-clean-ddd/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules:/mnt/c/Users/Pichau/Desktop/MBA/Projects/3. Desenvolvimento Back-end/04-clean-ddd/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Pichau/Desktop/MBA/Projects/3. Desenvolvimento Back-end/04-clean-ddd/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules/@biomejs/biome/bin/node_modules:/mnt/c/Users/Pichau/Desktop/MBA/Projects/3. Desenvolvimento Back-end/04-clean-ddd/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules/@biomejs/biome/node_modules:/mnt/c/Users/Pichau/Desktop/MBA/Projects/3. Desenvolvimento Back-end/04-clean-ddd/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules/@biomejs/node_modules:/mnt/c/Users/Pichau/Desktop/MBA/Projects/3. Desenvolvimento Back-end/04-clean-ddd/node_modules/.pnpm/@biomejs+biome@1.9.2/node_modules:/mnt/c/Users/Pichau/Desktop/MBA/Projects/3. Desenvolvimento Back-end/04-clean-ddd/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@biomejs/biome/bin/biome" "$@"
else
  exec node  "$basedir/../@biomejs/biome/bin/biome" "$@"
fi
